{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "(ctest) Launch",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "${command:cmake.launchTargetPath}",
			"args": [],
			"stopAtEntry": true,
			"cwd": "${workspaceRoot}",
			"environment": [
				{
					"name": "PATH",
					"value": "${env:PATH}:${command:cmake.getLaunchTargetDirectory}"
				}
			],
			"console": "internalConsole",
			"visualizerFile": "${workspaceRoot}/misc/natvis.xml"
		},
	]
}